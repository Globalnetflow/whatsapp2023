socket.on('connect', () => {
    socket.emit('join', { room: 'settings' });
});
socket.on('ready', () => {
    location.href = '/';
});
socket.on('wp-disconnect', () => {
    location.href = '/';
});

$(document).ready(function(){
    $(".loader").hide();
    $(".whatsapp-loader").hide();

    $("#cancel-disconnect").click(function(){
        $(".loader").hide();
        $( "#unlink-title" ).css( "background", "#2fcc71" );
    })
    $("#submit").click(function(){
        $("#unlink-button")[0].click();
    })
    if(!true && '' == '') {
        $( "#whatsapp-status" ).css( "pointer-events", "none" );
        $(".loader").show();
        
    }
});

function showUnlinkModal() {
    $( "#unlink-title" ).css( "background", "#d9534f" );
    $('#confirm-disconnect').modal({
        backdrop: 'static',
        keyboard: false
    })
    $('#confirm-disconnect').modal('show');
}
function spinnWP() {
    $( ".whatsapp-chat-nav-buttons" ).css( "pointer-events", "none" );
    $(".whatsapp-loader").show();
}
